@model MvcLabManager.Models.Computer; 

@{
    ViewData["Title"] = $"Computador {Model.Id}";
}

<h1>@ViewData["Title"]</h1>
<form action="/Computer/Update"><button class="btn btn-primary">Atualizar</button></form>
<hr>

<p>
    <b>Id:</b> @Model.Id
</p>
<p>
    <b>Ram:</b> @Model.Ram
</p>
<p>
    <b>Processador:</b> @Model.Processor
</p>

<form action="/Computer"><button class="btn btn-outline-primary">Voltar</button></form>